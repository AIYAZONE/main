{
	"name": "main",
	"private": true,
	"version": "0.1.0",
	"type": "module",
	"scripts": {
		"dev": "concurrently \"vite\" \"npm run docs:dev\"",
		"dev:main": "vite",
		"docs:dev": "vitepress dev docs --port 5174",
		"docs:build": "vitepress build docs",
		"docs:preview": "vitepress preview docs",
		"build": "vue-tsc -b && vite build && npm run docs:build && cp -r docs/.vitepress/dist/* dist/",
		"preview": "vite preview",
		"test": "vitest --run",
		"test:watch": "vitest",
		"test:ui": "vitest --ui"
	},
	"dependencies": {
		"element-plus": "^2.9.1",
		"gsap": "^3.14.2",
		"less": "^4.2.1",
		"pinia": "^2.3.0",
		"vue": "^3.5.13",
		"vue-i18n": "^9.14.5",
		"vue-router": "4"
	},
	"devDependencies": {
		"@types/node": "^25.0.1",
		"@types/vue": "^2.0.0",
		"@vitejs/plugin-vue": "^5.2.1",
		"@vitest/ui": "^4.0.15",
		"@vue/eslint-config-typescript": "^14.2.0",
		"@vue/test-utils": "^2.4.6",
		"@vue/tsconfig": "^0.7.0",
		"concurrently": "^9.2.1",
		"eslint": "^9.17.0",
		"eslint-plugin-vue": "^9.32.0",
		"fast-check": "^4.4.0",
		"happy-dom": "^20.0.11",
		"jsdom": "^27.0.1",
		"terser": "^5.44.1",
		"typescript": "~5.6.2",
		"vite": "^6.0.5",
		"vitepress": "^1.6.4",
		"vitest": "^4.0.15",
		"vue-tsc": "^2.2.0"
	}
}
