{
	"rewrites": [
		{ "source": "/fe", "destination": "/fe/index.html" },
		{ "source": "/fe/:path*", "destination": "/fe/:path*.html" },
		{ "source": "/pm", "destination": "/pm/index.html" },
		{ "source": "/pm/:path*", "destination": "/pm/:path*.html" },
		{ "source": "/blog", "destination": "/blog/index.html" },
		{ "source": "/blog/:path*", "destination": "/blog/:path*.html" },
		{ "source": "/(.*)", "destination": "/index.html" }
	]
}
